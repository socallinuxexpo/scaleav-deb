#!/opt/scaleavvenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'platformio==4.2.1','console_scripts','platformio'
__requires__ = 'platformio==4.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('platformio==4.2.1', 'console_scripts', 'platformio')()
    )
